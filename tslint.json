{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": ["node_modules"]
    },
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 2],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "one-line": false,
        "trailing-comma": false,
        "max-line-length": [true, 180],
        "no-console": false
    },
    "rulesDirectory": []
}
