    "source-map-support": "^0.5.10",
    "transform-loader": "0.2.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.13.1",
    "ts-loader": "^5.3.3",
    "typescript": "^2.9.2",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "@types/bufferstream": "^0.6.35",
    "@types/pdfkit": "^0.7.36",
